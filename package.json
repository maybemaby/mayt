{
  "name": "mayt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:db-up": "docker run --name maytdb -d -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=maytdb -p 5432:5432 postgres",
    "dev:db-push": "dotenv -e .env.dev -- prisma db push",
    "dev": "dotenv -e .env.dev -- next dev",
    "stage:db-migrate": "dotenv -e .env.staging -- prisma migrate deploy",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "dotenv -e .env.test -- jest --watch",
    "test:components": "jest components --watch",
    "test:db-up": "docker run --name testmaytdb -d -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=testmaytdb -p 5432:5432 postgres",
    "test:db-push": "dotenv -e .env.test -- primsa db push",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@prisma/client": "^4.1.0",
    "@trpc/client": "^9.26.2",
    "@trpc/next": "^9.26.2",
    "@trpc/react": "^9.26.2",
    "@trpc/server": "^9.26.2",
    "dotenv-cli": "^5.1.0",
    "lodash": "^4.17.21",
    "next": "12.1.6",
    "prisma": "^4.1.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.34.2",
    "react-hot-toast": "^2.3.0",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.4.0",
    "react-popper": "^2.3.0",
    "react-query": "^3.39.2",
    "react-youtube": "^9.0.3",
    "sharp": "^0.31.0",
    "styled-components": "^5.3.5",
    "zod": "^3.17.10",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "^5.1.25",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-testing-library": "^5.5.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.6.2",
    "typescript": "4.7.2"
  }
}
